<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.edu.zjut.dao.SingleDailyDao">
<!-- 根据打卡id来查询单个打卡的所有信息 --> 
<select id="getsinDailyByDID" resultType="cn.edu.zjut.po.SingleDaily" parameterType="cn.edu.zjut.po.SingleDaily">
	 SELECT * FROM singledaily
	 Where dailyID=#{dailyID}
</select> 
</mapper>