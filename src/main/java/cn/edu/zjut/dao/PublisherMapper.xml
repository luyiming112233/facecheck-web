<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.edu.zjut.dao.PublisherMapper">
      <select id="selectPublisherByPId" resultType="cn.edu.zjut.po.Publisher">
	SELECT
	PId as id, 
	name as name
	from Publisher
	WHERE PId = #{value} 
     </select> 
</mapper> 
