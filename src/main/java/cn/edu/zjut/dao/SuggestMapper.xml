<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.zjut.dao.SuggestMapper">

    <insert id="insertSuggest" parameterType="cn.edu.zjut.po.Suggest" useGeneratedKeys="true" keyProperty="suggestID"
            keyColumn="autoid">
            insert  into suggest_teacher (teacher_id,suggest,time) values
            (#{teacher.teaID},#{suggestion},#{date})
    </insert>
</mapper>
